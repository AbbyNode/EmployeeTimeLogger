<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.51" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.blueberrys.employeetimelogger.adminv.ControllerAV">
	<children>
		<MenuBar>
			<menus>
				<Menu mnemonicParsing="false" text="Menu">
					<items>
                  <MenuItem mnemonicParsing="false" onAction="#editAdminPass" text="Edit Admin Password" />
						<MenuItem mnemonicParsing="false" onAction="#exitToDaily" text="Exit to Daily View" />
					</items>
				</Menu>
			</menus>
		</MenuBar>
		<TabPane prefHeight="200.0" prefWidth="200.0" tabClosingPolicy="UNAVAILABLE" VBox.vgrow="ALWAYS">
			<tabs>
				<Tab fx:id="employeeTab" text="Employees" />
				<Tab fx:id="sessionTab" text="Sessions" />
            <Tab fx:id="reportTab" text="Report" />
			</tabs>
		</TabPane>
	</children>
</VBox>
